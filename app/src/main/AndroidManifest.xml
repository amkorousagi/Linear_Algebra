<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.linearalgebracomputer">
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".InputActivity_single_power"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".InputActivity_mult"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".InputActivity_plus"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".InputActivity_single"
            android:windowSoftInputMode="adjustPan"/>
        <activity android:name=".PastComputeActivity" />
        <activity android:name=".ComputeActivity" />
        <activity android:name=".Loading"/>
        <activity android:name=".MainActivity">

            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ResultActivity"
            android:label="Second 액티비티"/>
        <activity android:name=".ResultActivity2"/>
    </application>

</manifest>